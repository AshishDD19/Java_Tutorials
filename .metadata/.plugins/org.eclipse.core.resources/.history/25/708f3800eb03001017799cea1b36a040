package demo;

public class test {
	public static void main(String arg[]) {
		int arr[] = {1,2,3,4,5};
		for(int i=0;i<arr.length;i++) {
			System.out.print(arr[i]);
		}
		String str = "ashish";
		int count = 0;
		for(int i=0;i<str.length();i++) {
			if(str.charAt(i) == 's') {
				count++;	
			}
		}
		System.out.println(count);
		
		
				
		
		
			}
}
