import java.util.Scanner;
class CheckOddEven 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a value for n: ");
		int n = sc.nextInt();
		System.out.println("The first "+n+" Even no.s are: ");
		for(int i=2;i <= n*2;i += 2){
			System.out.print(i+" ");
		}
		System.out.println();
	}
}
